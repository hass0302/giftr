<?xml version='1.0' encoding='utf-8'?>
<widget id="edumedia.ca.hass0302.giftr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>giftr</name>
    <description> MAD9022 MIDTERM 2017 by Damian Ha</description>
    <author email="hass0302@algonquinlive.com" href="http://cordova.io"> Damian Ha</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="platforms/android/res/icons/android/ldpi.png" />
    <icon density="mdpi" src="platforms/android/res/icons/android/mdpi.png" />
    <icon density="hdpi" src="platforms/android/res/icons/android/hdpi.png" />
    <icon density="xhdpi" src="platforms/android/res/icons/android/xhdpi.png" />
    <icon density="xxhdpi" src="platforms/android/res/icons/android/xxhdpi.png" />
    <icon density="xxxhdpi" src="platforms/android/res/icons/android/xxxhdpi.png" />
    <splash density="land-hdpi" src="platforms/android/res/screens/android/splash-land-hdpi.png" />
    <splash density="land-ldpi" src="platforms/android/res/screens/android/splash-land-ldpi.png" />
    <splash density="land-mdpi" src="platforms/android/res/screens/android/splash-land-mdpi.png" />
    <splash density="land-xhdpi" src="platforms/android/res/screens/android/splash-land-xhdpi.png" />
    <splash density="land-xxhdpi" src="platforms/android/res/screens/android/splash-land-xxhdpi.png" />
    <splash density="land-xxxhdpi" src="platforms/android/res/screens/android/splash-land-xxxhdpi.png" />
    <splash density="port-hdpi" src="platforms/android/res/screens/android/splash-port-hdpi.png" />
    <splash density="port-ldpi" src="platforms/android/res/screens/android/splash-port-ldpi.png" />
    <splash density="port-mdpi" src="platforms/android/res/screens/android/splash-port-mdpi.png" />
    <splash density="port-xhdpi" src="platforms/android/res/screens/android/splash-port-xhdpi.png" />
    <splash density="port-xxhdpi" src="platforms/android/res/screens/android/splash-port-xxhdpi.png" />
    <splash density="port-xxxhdpi" src="platforms/android/res/screens/android/splash-port-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
