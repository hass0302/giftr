<?xml version='1.0' encoding='utf-8'?>
    <widget id="edumedia.ca.hass0302.giftr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
        <name>giftr</name>
        <description> MAD9022 MIDTERM 2017 by Damian Ha</description>
        <author email="hass0302@algonquinlive.com" href="http://cordova.io"> Damian Ha</author>
        <content src="index.html" />
        <preference name="SplashScreen" value="screen" />
        <preference name="SplashScreenDelay" value="5000" />
        <plugin name="cordova-plugin-whitelist" spec="1" />
        <access origin="*" />
        <allow-intent href="http://*/*" />
        <allow-intent href="https://*/*" />
        <allow-intent href="tel:*" />
        <allow-intent href="sms:*" />
        <allow-intent href="mailto:*" />
        <allow-intent href="geo:*" />
        <platform name="android">
            <allow-intent href="market:*" />
            <icon src="platforms/android/res/icons/android/ldpi.png" density="ldpi" />
            <icon src="platforms/android/res/icons/android/mdpi.png" density="mdpi" />
            <icon src="platforms/android/res/icons/android/hdpi.png" density="hdpi" />
            <icon src="platforms/android/res/icons/android/xhdpi.png" density="xhdpi" />
            <icon src="platforms/android/res/icons/android/xxhdpi.png" density="xxhdpi" />
            <icon src="platforms/android/res/icons/android/xxxhdpi.png" density="xxxhdpi" />
            <splash src="platforms/android/res/screens/android/splash-land-hdpi.png" density="land-hdpi" />
            <splash src="platforms/android/res/screens/android/splash-land-ldpi.png" density="land-ldpi" />
            <splash src="platforms/android/res/screens/android/splash-land-mdpi.png" density="land-mdpi" />
            <splash src="platforms/android/res/screens/android/splash-land-xhdpi.png" density="land-xhdpi" />
            <splash src="platforms/android/res/screens/android/splash-land-xxhdpi.png" density="land-xxhdpi" />
            <splash src="platforms/android/res/screens/android/splash-land-xxxhdpi.png" density="land-xxxhdpi" />
            <splash src="platforms/android/res/screens/android/splash-port-hdpi.png" density="port-hdpi" />
            <splash src="platforms/android/res/screens/android/splash-port-ldpi.png" density="port-ldpi" />
            <splash src="platforms/android/res/screens/android/splash-port-mdpi.png" density="port-mdpi" />
            <splash src="platforms/android/res/screens/android/splash-port-xhdpi.png" density="port-xhdpi" />
            <splash src="platforms/android/res/screens/android/splash-port-xxhdpi.png" density="port-xxhdpi" />
            <splash src="platforms/android/res/screens/android/splash-port-xxxhdpi.png" density="port-xxxhdpi" /> </platform>
        <platform name="ios">
            <allow-intent href="itms:*" />
            <allow-intent href="itms-apps:*" /> </platform>
        <engine name="android" spec="~6.0.0" /> </widget>